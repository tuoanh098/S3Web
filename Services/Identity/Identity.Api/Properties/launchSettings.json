{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Identity (http)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5001",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Jwt__Key": "dev-identity-signing-key-please-change",
        "Jwt__Issuer": "dev-issuer",
        "Jwt__Audience": "dev-audience",
        "Internal__ServiceToken": "dev-internal-token-CHANGE_ME"
      }
    },
    "Identity (https)": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:5003;http://localhost:5001",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "Jwt__Key": "dev-identity-signing-key-please-change",
        "Jwt__Issuer": "dev-issuer",
        "Jwt__Audience": "dev-audience",
        "Internal__ServiceToken": "dev-internal-token-CHANGE_ME"
      }
    }
  }
}
